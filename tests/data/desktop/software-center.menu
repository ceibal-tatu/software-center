    </Include>
  </Menu> 

  <!-- multimedia -->
  <Menu>
    <Name>Dynamic</Name>
    <Include>
      <And>
        <Category>AudioVideo</Category>
      </And>
    </Include>
  </Menu> 


</Menu> 
